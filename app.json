{
  "expo": {
    "name": "EduHub",
    "slug": "eduhub",
    "scheme": "eduhub",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {},
    "web": {
      "bundler": "webpack"
    },
    "plugins": [
      "expo-localization",
      "expo-font"
    ]
  }
}
